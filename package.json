    "url": "git+https://github.com/nimi/minimalist-frontend-template.git"
  },
  "keywords": [
    "minimal",
    "esbuild",
    "esinstall",
    "snowpack"
  ],
  "author": "Nicholas Mitchell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nimi/minimalist-frontend-template/issues"
  },
  "homepage": "https://github.com/nimi/minimalist-frontend-template#readme",
  "dependencies": {
    "dom-confetti": "0.2.2"
  },
  "esinstall": {
